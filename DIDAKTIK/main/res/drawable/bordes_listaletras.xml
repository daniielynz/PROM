<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="150dip" />
            <stroke android:width="6dip" android:color="@color/white" />
            <solid android:color="@color/masOscuro" />
            <padding
                android:left="30dp"
                android:top="20dp"
                android:right="30dp"
                android:bottom="20dp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="150dip" />
            <stroke android:width="6dip" android:color="@color/masOscuro" />
            <solid android:color="@color/white" />
            <padding
                android:left="30dp"
                android:top="20dp"
                android:right="30dp"
                android:bottom="20dp"/>
        </shape>
    </item>
</selector>
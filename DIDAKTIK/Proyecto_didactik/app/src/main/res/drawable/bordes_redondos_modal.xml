<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Primer elemento: Rectángulo con borde ancho y esquinas redondeadas, color tomado de @color/masOscuro -->
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <solid android:color="@color/masOscuro" />
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <!-- Segundo elemento: Rectángulo con borde más delgado y esquinas redondeadas, color tomado de @color/oscuro -->
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />
            <solid android:color="@color/oscuro" />
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <!-- Tercer elemento: Rectángulo con borde asimétrico y esquinas redondeadas, color tomado de @color/masOscuro -->
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="4dp"
                android:left="2dp"
                android:right="2dp"
                android:top="4dp" />
            <solid android:color="@color/masOscuro" />
            <corners android:radius="10dp"/>
        </shape>
    </item>

    <!-- Cuarto elemento: Rectángulo con borde ancho y esquinas redondeadas, color tomado de @color/masClaro -->
    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <solid android:color="@color/masClaro" />
            <corners android:radius="10dp"/>
        </shape>
    </item>

</layer-list>